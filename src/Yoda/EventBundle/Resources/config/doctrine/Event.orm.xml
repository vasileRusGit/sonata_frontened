<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Yoda\EventBundle\Entity\Event" table="event">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="product_name" type="string" column="product_name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="car_mark" type="string" column="car_mark" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="car_model" type="string" column="car_model" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="car_year" type="string" column="car_year" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="stock" type="boolean" column="stock" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="details" type="string" column="details" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cover" type="string" column="cover" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="deleted" type="boolean" column="deleted" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
